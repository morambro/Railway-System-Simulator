project TrainSimulation is

   for Object_Dir use "out";
   for Source_Dirs use ("src/**");

   package Make is
      for Makefile use "Makefile";
   end Make;

   package Compiler is
      for Default_Switches ("ada") use ("-gnatf");
   end Compiler;

   package Pretty_Printer is
      for Default_Switches ("ada") use ("-i4");
   end Pretty_Printer;

end TrainSimulation;

